alt + shift + q
  bspc quit

alt + shift + c
  bspc window -c

alt + shift + Return
  urxvtc

alt + p
  interrobang

alt + {_,shift + }{h,j,k,l}
  bspc window -{f,s} {left,down,up,right}

alt + {_,shift + }{1-6}
  bspc {desktop -f,window -d} ^{1-6}

alt + {space,f}
  bspc window -t {floating,fullscreen}

# 1 ".Xresources"
# 1 "<command-line>"
# 1 ".Xresources"
! Fonts

Xft.dpi: 96
Xft.autohint: 0
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.antialias: 1
Xft.lcdfilter: lcddefault
Xft.rgba: rgb


! Urxvt

URxvt.font:xft:DejaVu Sans Mono-10
URxvt.boldFont:xft:DejaVu Sans Mono-10:style=bold
URxvt.letterSpace: -1

URxvt*scrollBar: false
URxvt*mouseWheelScrollPage: True

URxvt*internalBorder: 10
URxvt*externalBorder: 0

URxvt*urgentOnBell: true

URxvt.intensityStyles: false

URxvt.fading: 30

! solarized light colorscheme
*background: #fdfaf6
*foreground: #657b83
*fadeColor: #fdfaf6
*cursorColor: #586e75
*pointerColorBackground: #93a1a1
*pointerColorForeground: #586e75

*color0: #eee8d5
*color1: #dc322f
*color2: #859900
*color3: #b58900
*color4: #268bd2
*color5: #d33682
*color6: #2aa198
*color7: #073642
*color9: #cb4b16

! Fonts

Xft.dpi: 96


! Urxvt

URxvt.font:xft:terminus:size=20
URxvt.boldFont:xft:terminus:bold:size=20

URxvt*scrollBar: false
URxvt*mouseWheelScrollPage: True

URxvt*internalBorder: 10
URxvt*externalBorder: 0

URxvt*urgentOnBell: true

URxvt.intensityStyles: false

URxvt.fading: 30

! gruvbox dark colorscheme

*fadeColor: #282828
*cursorColor: #928374

! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
